{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./dist",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run clean && npm run typecheck",
    "build": "node build.mjs",
    "start": "node dist/index.mjs",
    "dev": "tsx watch src",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "drizzle:gen": "drizzle-kit generate:sqlite",
    "drizzle:migrate": "tsx src/db/migrate.ts",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "drizzle:drop": "drizzle-kit drop"
  },
  "dependencies": {
    "@libsql/client": "^0.5.1",
    "@lucia-auth/adapter-drizzle": "^1.0.2",
    "@sendgrid/mail": "^8.1.1",
    "@trpc/client": "11.0.0-next-beta.289",
    "@trpc/server": "11.0.0-next-beta.289",
    "@types/cookie-parser": "^1.4.6",
    "@upstash/ratelimit": "^1.2.0",
    "@upstash/redis": "^1.28.4",
    "arctic": "^1.2.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.0",
    "express": "^4.18.2",
    "lucia": "^3.2.0",
    "meilisearch": "^0.40.0",
    "oslo": "^1.1.2",
    "tsx": "^4.7.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "drizzle-kit": "^0.22.1",
    "esbuild": "^0.21.5",
    "esbuild-plugin-file-path-extensions": "^2.1.2",
    "eslint": "^8.56.0",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": "./src/index.ts"
  }
}
